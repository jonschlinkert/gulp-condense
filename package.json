{
  "name": "gulp-condense",
  "description": "Remove extra newlines in a string.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/gulp-condense",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-condense",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-condense/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "condense-newlines": "^0.2.0",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.4.5",
    "vinyl": "^1.1.1"
  },
  "keywords": [
    "condense",
    "gulp",
    "gulpplugin",
    "newline",
    "plugin",
    "reduce",
    "remove",
    "stream",
    "string",
    "strip"
  ],
  "verb": {
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "verb"
    ],
    "layout": "default",
    "toc": false,
    "tasks": [
      "readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}